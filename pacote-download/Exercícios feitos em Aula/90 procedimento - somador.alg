algoritmo "procedimento - somador"
var
   soma, a, b: inteiro
   Procedimento Somador()
   inicio
      soma <- a + b
   Fimprocedimento

inicio
      Escreva("Digite um valor inteiro: ")
      Leia(a)
      Escreva("Digite um valor inteiro: ")
      Leia(b)
      Somador()
      Escreva("A soma entre os dois valores é: ", soma)
fimalgoritmo

algoritmo "Conte até q valor"
var
   Contador, N: inteiro
inicio
      Contador <- 0
      Escreval("Deseja contar até qual numero?")
      Leia(N)
      Enquanto (Contador >= 0) e (Contador < N) faca
               Contador <- Contador + 1
               escreval (Contador)
      FimEnquanto
               Escreval("Acabou!")
      
fimalgoritmo
